/**index.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 170rpx 0;
  box-sizing: border-box;

} 
.swiper-mt{
  display: flex;
  justify-content: center;
  align-items: center;
}
.screen-swiper{
  width: 99%;
  border-radius: 30rpx;
  overflow: hidden;
}

.box {
  width: 98%;
  border-radius: 20rpx;
  margin-left: auto;
  margin-right: auto;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, .1);
}

.van-notice-bar{
  border-radius: 20rpx;
}


.fix_btn {
  width: 85rpx;
  height: 100rpx;
  position: fixed;
  z-index: 10;
  bottom: 220rpx;
  right: 5rpx;
}
 
.icon {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.tabs-mt{
  width: 98%;
  margin-left: auto;
  margin-right: auto;
  overflow:hidden;
  border: 1px solid red;
  border-radius: 20rpx !important;
  background-color: #fff;
}
